<link href="/stylesheets/selectPlanet.css" rel="stylesheet"/>
<main class="container">
    <!-- Nameplate area for displaying top right title screen text. -->
    <section id="details">
        <div id="planets">
            <div id="planet1">
                <h4 id="planetName1">
                    {{results.dataValues.Race.dataValues.Planet.dataValues.planet_name}}
                </h4>
                <p id="travelPrice1">
                    travelprice 1
                </p>
                <button id="button1" type="button" class="nes-btn is-primary"></button>
            </div>
            <div id="planet2">
                <h4 id="planetName2">{{results.dataValues.Race.dataValues.Planet.dataValues.planet_name}}</h4>
                <p id="travelPrice2">travelprice 2</p>
                <button id="button2" type="button" class="nes-btn is-primary"></button>
            </div>
            <div id="planet3">
                <h4 id="planetName3">{{results.dataValues.Race.dataValues.Planet.dataValues.planet_name}}</h4>
                <p id="travelPrice3">travelprice 3</p>
                <button id="button3" type="button" class="nes-btn is-primary"></button>
            </div>
        </div>
        <div id="information">
            <div id="ships">ships</div>
            {{!-- <h3 id="fuel">Currently fuel level:</h3>
            <h3 id="fuelValue"></h3>
            <h3 id="fromHome">Days way from home:</h3>
            <h3 id="fromHomeValue"></h3> --}}
            <h2 id="description">Select a planet to travel to..</h2>
        </div>
        <div class="travel-planets">
            <h3 class="planetItem">{{results.dataValues.Planet.dataValues.travel1}}</h3>
            <h3 class="planetItem">{{results.dataValues.Planet.dataValues.travel2}}</h3>
            <h3 class="planetItem">{{results.dataValues.Planet.dataValues.travel3}}</h3>
        </div>
    </section>
</main>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script type="text/javascript" src="/js/selectPlanet.js"></script>