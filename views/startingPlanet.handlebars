<link href="/stylesheets/startingPlanet.css" rel="stylesheet"/>
<body class="container">
    <!-- Nameplate area for displaying top right title screen text. -->
    <header id="details">
        <div id="planetInfo">
            <h3 class="title">Currently playing on</h3>
            <h2 id="planet">{{results.dataValues.Race.dataValues.Planet.dataValues.planet_name}}</h3>
        </div>
        <div id="characterInfo">
            <h3 class="title">Currently playing as</h3>
            <h1 id="name">{{results.dataValues.char_name}}</h1>
        </div>
        <div id="professionInfo">
            <h3 id="title">Currently playing as</h3>
            <h1 id="profession">{{results.dataValues.Profession.dataValues.profession}}</h1>
        </div>
    </header>
    <main id="content">
        <div id="ships">

        </div>
        <div id="play">

        </div>
        <div id="instructions">

        </div>
    </main>
</body>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script type="text/javascript" src="/js/startingPlanet.js"></script>